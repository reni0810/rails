.nested-fields.container
  = f.label :date
  .form-inline
    = f.date_select :date,{start_year: Time.now.year,end_year:2030},{class: "form-control"}
  .form-inline
    %table.table-responsive
      %tr
        %td
          = f.label :start_time
          = f.time_select :start_time, {ampm: true},class: "form-control"
        %td
          = f.label :end_time
          = f.time_select :end_time,{ampm: true}, class: "form-control"
  %p
    Full day
    \#{f.check_box :full_day}
  = link_to_remove_association "remove date", f
