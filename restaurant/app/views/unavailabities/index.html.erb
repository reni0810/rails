<p id="notice"><%= notice %></p>

<h1>Unavailabities</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Full day</th>
      <th>Restaurant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unavailabities.each do |unavailabity| %>
      <tr>
        <td><%= unavailabity.date %></td>
        <td><%= unavailabity.start_time %></td>
        <td><%= unavailabity.end_time %></td>
        <td><%= unavailabity.full_day %></td>
        <td><%= unavailabity.restaurant %></td>
        <td><%= link_to 'Show', unavailabity %></td>
        <td><%= link_to 'Edit', edit_unavailabity_path(unavailabity) %></td>
        <td><%= link_to 'Destroy', unavailabity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unavailabity', new_unavailabity_path %>
